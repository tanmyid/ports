name=markupsafe
version=1.1.1
release=1
source=(https://files.pythonhosted.org/packages/source/M/MarkupSafe/MarkupSafe-$version.tar.gz)

build() {
	cd MarkupSafe-$version

	python3 setup.py build
	python3 setup.py install --root=$PKG --optimize=1

}
